LINK="\$(CC)"

MTR_INCLUDE_OPT="-I "
MTR_COMPILE_OPT="-c "
MTR_OBJOUT="-o "
MTR_BINOUT="-o "
MTR_OBJEXT="o"
MTR_INCEXT="h"
MTR_BINEXT=""

. auto/cc/name

case $MTR_CC_NAME in
    gcc)
        . auto/cc/gcc
    ;;
    unknown)
        echo
        echo $0: error: C compiler $CC is not found
        echo
        exit 1
    ;;
esac
